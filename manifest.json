{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "4",
    "homepage_url": "https://github.com/kyuucr/open-link-in-other-window",
    "default_locale": "en",
    "applications": {
        "gecko": {
            "id": "{33c9a048-7643-4e7c-839d-870b65edb9f8}",
            "strict_min_version": "48.0"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
          "page": "options.html",
          "browser_style": true
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "tabs"
    ]
}
