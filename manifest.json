{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "2",
    "homepage_url": "https://github.com/kyuucr/open-link-in-other-window",
    "default_locale": "en",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
          "page": "options.html",
          "browser_style": true
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "tabs"
    ]
}
